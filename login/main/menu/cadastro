/*
 * To change this license header, choose License Headers in Project Properties.
 * To change this template file, choose Tools | Templates
 * and open the template in the editor.
 */
package view;

import Model.Agendamento;
import Model.Cliente;
import Model.Pessoa;
import Model.Serviço;
import Model.Usuario;

/**
 *
 * @author pc geral
 */
public class Main {
    
    
    public static void main(String[] args){
        
        String nome = "Thiago";
        System.out.println(nome);
        
        Serviço serviço = new Serviço(1,"medicamentos", 25);
        
        System.out.println(serviço.getDescricao());
        System.out.println(serviço.getValor());
        
        Cliente cliente = new Cliente(1, "kaue", "rua fernandes", "1452678");
        System.out.println(cliente.getNome());
        
        Usuario usuario = new Usuario(1, "agendamento", "senha");
        System.out.println(usuario.getNome());
        
        Agendamento agendamento = new Agendamento(1, cliente, serviço, 25, "11/09/2020 11:30"); 
        System.out.println(agendamento.getCliente().getNome());
    }
}
